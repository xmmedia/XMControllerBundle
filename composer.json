    "type": "symfony-bundle",
    "homepage": "https://github.com/xmmedia/XMControllerBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Hein",
            "email": "dhein@xmmedia.com"
        }
    ],
    "require": {
